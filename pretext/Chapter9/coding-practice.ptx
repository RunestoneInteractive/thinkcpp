#include <iostream>
using namespace std;

struct Shirt {
    string color;
    char size;
};

struct Pants {
    char size;
    string material;
};

struct Outfit {
    Shirt s;
    Pants p;
    bool has_hat;
}

void printOutfit (Outfit o) {
"Shirt: blue and L; Pants: S and denim; has hat"
    cout << "Shirt: " << o.s.color << " and " << o.s.size << "; Pants:" << o.p.size << " and " << o.p.material << "; ";
    if (o.has_hat) {
        cout << "has hat" << endl;
    }
    else {
        cout << "does not have hat" << endl;
    }
}

int main() {
    Shirt t = { "blue", 'L' };
    Pants p = { 'S', "denim" };
    Outfit o = { t, p, true };
    printOutfit (o);
}
