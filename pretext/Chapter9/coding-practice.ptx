    <program xml:id="cp_9_AC_5a" line-numbers="yes"><input><![CDATA[
#include <iostream>
using namespace std;

struct Cake {
    string name;
    string color;
    double diameter;
    bool has_icing;
};

void printCakeInfo (Cake c);
Cake makeCake ();

void editCake (Cake& c) {
    Cake newCake = makeCake ();
    c = newCake;
}

int main() {
    Cake original = { "Oops", "orange", 185, true };
    editCake (original);
    printCakeInfo (original);
}
        ]]></input><tests>
